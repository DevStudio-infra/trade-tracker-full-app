# Capital.com API credentials
# You need to set up a real account or demo account at Capital.com
# Then generate an API key in the Settings > API integrations section
CAPITAL_API_KEY=your_api_key_here
CAPITAL_USERNAME=your_username_here
CAPITAL_PASSWORD=your_password_here
CAPITAL_DEMO_MODE=true

# Other application settings
# Add other environment variables below
PORT=3000

# Supabase settings (for chart storage)
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key
