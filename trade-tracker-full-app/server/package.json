{
  "name": "expressjs-server",
  "version": "1.0.0",
  "private": true,
  "description": "The backend of the Next.js-Express-Auth0 integration template. It's built using Express.js and provides API endpoints, authentication routes, and middleware configurations.",
  "homepage": "https://benjamin-chavez.com/blog/integrating-next.js-with-express.js-using-auth0-for-authentication",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node src/index.ts",
    "build": "tsc",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:migrate:dev": "cross-env NODE_ENV=development tsx scripts/migrate.ts"
  },
  "keywords": [
    "example",
    "express",
    "auth0",
    "template",
    "backend",
    "api",
    "typescript",
    "node"
  ],
  "author": {
    "name": "Ben Chavez",
    "email": "ben.m.chavez@gmail.com",
    "url": "https://benjamin-chavez.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benjamin-chavez/nextjs-express-auth0-template"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.15.0"
  },
  "dependencies": {
    "@ai-sdk/google": "1.1.17",
    "@auth/core": "^0.38.0",
    "@auth/prisma-adapter": "^2.8.0",
    "@google/generative-ai": "0.22.0",
    "@prisma/client": "6.4.1",
    "@react-email/components": "0.0.33",
    "@react-email/render": "1.0.5",
    "@supabase/supabase-js": "2.49.1",
    "@types/jsonwebtoken": "^9.0.9",
    "ai": "4.1.47",
    "axios": "1.8.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-async-handler": "1.2.0",
    "express-flash": "0.0.2",
    "ioredis": "5.5.0",
    "jose": "^6.0.8",
    "jsonwebtoken": "^9.0.2",
    "morgan": "1.10.0",
    "next-auth": "^4.24.11",
    "openai": "4.86.1",
    "react-email": "3.0.7",
    "resend": "4.1.2",
    "stripe": "17.7.0",
    "svix": "1.60.1",
    "uuid": "11.1.0",
    "ws": "8.18.1",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.20",
    "@types/express-flash": "0.0.2",
    "@types/morgan": "1.9.5",
    "@types/node": "20.5.9",
    "cross-env": "7.0.3",
    "nodemon": "3.0.1",
    "prisma": "6.4.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
